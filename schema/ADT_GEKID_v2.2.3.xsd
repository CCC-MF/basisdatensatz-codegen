										<xs:annotation>
											<xs:documentation>Haut</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="OTH">
										<xs:annotation>
											<xs:documentation>Andere Organe</xs:documentation>
