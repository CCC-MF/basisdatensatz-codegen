								<xs:documentation>Undifferenziert</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="X">
							<xs:annotation>
								<xs:documentation>Nicht bestimmbar</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="L">
							<xs:annotation>
								<xs:documentation>Low grade (G1 oder G2)</xs:documentation>
